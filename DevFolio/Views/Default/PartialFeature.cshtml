@model List<DevFolio.Models.TblFeature>

@foreach (var item in Model)
{
    <div id="home" class="intro route bg-image" style="background-image: url(/Templates/devfolio-master/img/intro-bg.jpg)">
        <div class="overlay-itro"></div>
        <div class="intro-content display-table">
            <div class="table-cell">
                <div class="container">
                    
                    <h1 class="intro-title mb-4">@item.HeaderTitle</h1>
                    <p class="intro-subtitle"><span class="text-slider-items">@item.HeaderDescription</span><strong class="text-slider"></strong></p>
                    
                </div>
            </div>
        </div>
    </div>
}